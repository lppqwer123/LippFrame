<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DotView">
        <attr name="dot_radius" format="dimension" />
        <attr name="dot_span" format="dimension" />
        <attr name="dot_src" format="reference" />
    </declare-styleable>

    <declare-styleable name="TagFlowLayout">
        <attr name="max_select" format="integer"></attr>
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="StickyScrollView">
        <attr name="stuckShadowHeight" format="dimension" />
        <attr name="stuckShadowDrawable" format="reference" />
    </declare-styleable>
</resources>